    "@vitest/ui": "4.0.14",
    "autoprefixer": "10.4.22",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "10.1.8",
